<script>
$(document).on("change", "#months", function() {
    var SelectedText = $(this).find("option:selected").text();
     var SelectedValue = $(this).find("option:selected").val();
    alert('Selected Value :' + SelectedValue+ '\n Selected Text :' + SelectedText );
    
});
</script>
